import React from 'react';

function PlaylistContent({ playlist }) {
  return (
    <div>
      {playlist ? (
        <div>
          <h3>{mixtape.name}</h3>
          <ul>
            {mixtape.songs.map((song, index) => (
              <li key={index}>{song}</li>
            ))}
          </ul>
        </div>
      ) : (
        <p>Please select a mixtape to view songs.</p>
      )}
    </div>
  );
}

export default PlaylistContent;
